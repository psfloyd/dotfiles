###---Symbol Bindings---###{{{
bindsym $mod+Escape		workspace prev
#bindsym $mod+Shift+Escape 

bindsym $mod+F1			workspace next
#bindsym $mod+Shift+F1

bindsym $mod+F2		exec --no-startup-id python ~/.config/Scripts/shortcuts.py
#bindsym $mod+Shift+F2


#bindsym $mod+F3
#bindsym $mod+Shift+F3

bindsym $mod+F4		mode "$mode_screen"	
bindsym $mod+Shift+F4	exec --no-startup-id arandr

#bindsym $mod+F5		
#bindsym $mod+Shift+F5		

bindsym $mod+F6		exec $multifuncsh compton
#bindsym $mod+Shift+F6		

bindsym $mod+F7		exec transset-df -a --dec .15
#bindsym $mod+Shift+F7		

bindsym $mod+F8		exec transset-df -a --inc .15
#bindsym $mod+Shift+F8		

#bindsym $mod+F9	
#bindsym $mod+Shift+F9		

#bindsym $mod+F10	
#bindsym $mod+Shift+F10		

#bindsym $mod+F11	
#bindsym $mod+Shift+F11		

#bindsym $mod+F12	
#bindsym $mod+Shift+F12		

bindsym Print 				exec --no-startup-id $multifuncsh screenshot
bindsym Shift+Print --release		exec --no-startup-id $multifuncsh screenshot area

#bindsym $mod+Delete
bindsym $mod+Shift+Delete	exec $truemute ; exec $truepause ; workspace lmao ; exec $term -e htop ; exec $term -e ranger

bindsym $mod+grave		exec $flash
#bindsym $mod+Shift+grave

#bindsym $mod+minus
#bindsym $mod+Shift+minus

#bindsym $mod+equal
#bindsym $mod+Shift+equal

bindsym $mod+BackSpace		exec $flash
#bindsym $mod+Shift+BackSpace	exec sudo reboot

bindsym $mod+Tab		workspace back_and_forth
#bindsym $mod+Shift+Tab

#bindsym $mod+bracketleft
#bindsym $mod+Shift+bracketleft

#bindsym $mod+bracketright
#bindsym $mod+Shift+bracketright

#bindsym $mod+backslash
#bindsym $mod+Shift+backslash

#bindsym $mod+semicolon		workspace next
#bindsym $mod+Shift+semicolon

bindsym $mod+apostrophe		split horizontal ;; exec $term
#bindsym $mod+Shift+apostrophe

bindsym $mod+Return 		exec $term
bindsym $mod+Shift+Return 	exec $term -e $tmux

bindsym $mod+comma	 	focus parent
#bindsym $mod+Shift+comma

bindsym $mod+period	 	focus child
#bindsym $mod+Shift+period

bindsym $mod+slash		split vertical ;; exec $term
#bindsym $mod+Shift+slash

bindsym $mod+Shift+space 	floating toggle
bindsym $mod+space		focus mode_toggle

# }}}

