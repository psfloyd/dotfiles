###---Misc Bindings---###{{{
bindsym $mod+Home		workspace $ws1
bindsym $mod+Shift+Home		move container to workspace $ws1

bindsym $mod+End		workspace $ws10
bindsym $mod+Shift+End		move container to workspace $ws10

bindsym $mod+Prior		workspace prev
bindsym $mod+Shift+Prior	move container to workspace prev

bindsym $mod+Next		workspace next
bindsym $mod+Shift+Next		move container to workspace next

# }}}

###---Letter Key Bindings---###{{{
bindsym $mod+q			kill
bindsym $mod+Shift+q		kill

bindsym $mod+w			exec --no-startup-id $browser
#bindsym $mod+w			exec $multifuncsh browser
#bindsym $mod+Shift+w		

#bindsym $mod+e			
#bindsym $mod+Shift+e

bindsym $mod+r 			exec $multifuncsh ranger
bindsym $mod+Shift+r		exec $restart

bindsym $mod+t			split toggle
#bindsym $mod+Shift+t		

#bindsym $mod+y			
bindsym $mod+Shift+y		resize shrink width 10 px or 10ppt

bindsym $mod+u			exec $multifuncsh tmux
bindsym $mod+Shift+u		resize shrink height 10 px or 10 ppt

bindsym $mod+i 			exec $term -e htop
bindsym $mod+Shift+i		resize grow height 10 px or 10 ppt

#bindsym $mod+o			
bindsym $mod+Shift+o		resize grow width 10 px or 10 ppt

#bindsym $mod+p			
bindsym $mod+Shift+p		exec mypaint

bindsym $mod+a			gaps inner current set 15; gaps outer current set 15
bindsym $mod+Shift+a		gaps inner current set 0; gaps outer current set 0

bindsym $mod+s			gaps inner current plus 5
bindsym $mod+Shift+s		gaps inner current minus 5

bindsym $mod+d			exec $multifuncsh rofi
bindsym $mod+Shift+d		exec $multifuncsh rofi gksudo

bindsym $mod+f			fullscreen toggle
#bindsym $mod+Shift+f

#bindsym $mod+g			workspace prev
#bindsym $mod+Shift+g		

bindsym $mod+h			focus left
bindsym $mod+Shift+h		move left 30

bindsym $mod+j			focus down
bindsym $mod+Shift+j		move down 30

bindsym $mod+k			focus up
bindsym $mod+Shift+k		move up 30

bindsym $mod+l			focus right
bindsym $mod+Shift+l		move right 30

bindsym $mod+z			gaps outer current plus 5
bindsym $mod+Shift+z		gaps outer current minus 5

bindsym $mod+x	        	mode "$mode_system"
bindsym $mod+Shift+x		exec $lock

bindsym $mod+c			mode "$mode_layout"
#bindsym $mod+Shift+c

#bindsym $mod+v			
#bindsym $mod+Shift+v

#bindsym $mod+b			
#bindsym $mod+Shift+b	

#bindsym $mod+n			
#bindsym $mod+Shift+n

bindsym $mod+m 			mode "$mode_mtp"
bindsym $mod+Shift+m		exec $mute
# }}}

###---Media Keys---###{{{
#Control keys
bindsym $mod1+z			exec $multifuncsh music
bindsym $mod1+a			exec $prev 
bindsym $mod1+s			exec $pause 
bindsym $mod1+d			exec $next 
bindsym $mod1+x			exec $stop 
bindsym $mod1+c			exec $crop 

#Volume keys

bindsym $mod1+plus		exec $musinc
bindsym $mod1+Shift+plus	exec $musbiginc

bindsym $mod1+minus 		exec $musdec
bindsym $mod1+Shift+minus	exec $musbigdec

# }}}

